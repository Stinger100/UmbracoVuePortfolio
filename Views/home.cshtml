@inherits Umbraco.Web.Mvc.UmbracoViewPage<Home>
@{
	Layout = "~/Views/Shared/layout.cshtml";
	string mainImageUrl = Model.Value<IPublishedContent>("mainImage").Url;
	string avatarImage = Model.Value<IPublishedContent>("avatarImage").Url;
	string devImage= Model.Value<IPublishedContent>("devImage").Url;
	}
<div  class="container" id="home">
<div class="header">
		<div class="logo-image">
	   <a href="/"><img src="@mainImageUrl"></a> 
	</div>
	<div class="navbar">
			@Umbraco.RenderMacro("NavBar")
	</div>
	</div>
	<div class="text-card">
		    <div class="text-card_title">
		        @Model.Value("title")
		    </div>
		    <div class="text-card_description">
		    @Model.Value("description")
		   </div>
	</div>	
	<div class="avatar-image">
	    <img src="@avatarImage">
	</div>
	<div class="dev-image">
	    <img src="@devImage">
	</div>
		

</div>

<script>
	new Vue({
		el: '#home',
		data () {
			return {
			
			}
		}
	})
</script>

